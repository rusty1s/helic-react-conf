{
  "name": "helic-react-config",
  "version": "0.1.0",
  "description": "Customizable Configuration for modern React apps",
  "keywords": [
    "configuration",
    "scripts",
    "build",
    "lint",
    "test",
    "watch",
    "react",
    "css-modules",
    "postcss",
    "eslint",
    "stylelint",
    "webpack",
    "jest"
  ],
  "bugs": {
    "url": "https://github.com/rusty1s/helic-react-conf/issues",
    "email": "matthias.fey@tu-dortmund.de"
  },
  "license": "MIT",
  "author": {
    "name": "Matthias Fey",
    "email": "matthias.fey@tu-dortmund.de"
  },
  "files": [
    "bin",
    "scripts",
    "config",
    "utils",
    "mocks",
    ".babelrc"
  ],
  "bin": {
    "react-config": "bin/react-config.js"
  },
  "directories": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rusty1s/helic-react-conf"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "lint": "eslint --config config/eslintrc --ignore-path config/eslintignore --fix --cache --ext .js --ext .jsx .",
    "test": "jest --coverage",
    "demo:install": "cd demo && npm install",
    "demo:uninstall": "cd demo && npm uninstall",
    "demo:lint": "cd demo && npm run lint",
    "demo:test": "cd demo && npm test",
    "demo:build": "cd demo && npm run build"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chalk": "^1.1.3",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^17.0.3",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "svn-modules": "^0.1.8",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "peerDependencies": {
    "enzyme": "^2.0.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "demo"
    ]
  }
}
