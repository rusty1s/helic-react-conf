{
  "name": "react-dev-config",
  "version": "2.2.1",
  "description": "Customizable Configuration for modern React apps",
  "keywords": [
    "development",
    "configuration",
    "scripts",
    "build",
    "lint",
    "test",
    "watch",
    "react",
    "css-modules",
    "postcss",
    "eslint",
    "stylelint",
    "webpack",
    "jest"
  ],
  "bugs": {
    "url": "https://github.com/rusty1s/react-dev-config/issues",
    "email": "matthias.fey@tu-dortmund.de"
  },
  "license": "MIT",
  "author": {
    "name": "Matthias Fey",
    "email": "matthias.fey@tu-dortmund.de"
  },
  "files": [
    "bin",
    "scripts",
    "config",
    "utils",
    "__mocks__",
    "babelrc.js",
    "eslintignore.js",
    "eslintrc.js",
    "jest.js",
    "postcss.js",
    "stylelintignore.js",
    "stylelintrc.js",
    "webpack.common.js",
    "webpack.dev.js",
    "webpack.prod.js"
  ],
  "bin": {
    "react-dev-config": "bin/react-dev-config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rusty1s/react-dev-config"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "lint": "eslint --config eslintrc.js --ignore-path .gitignore --cache --ext .js --ext .jsx .",
    "test": "jest --coverage",
    "demo:install": "cd demo && npm install",
    "demo:uninstall": "cd demo && npm uninstall",
    "demo:lint": "cd demo && npm run lint",
    "demo:test": "cd demo && npm test",
    "demo:build": "cd demo && npm run build",
    "demo:transpile": "cd demo && npm run transpile"
  },
  "pre-commit": [
    "test"
  ],
  "pre-push": [
    "lint"
  ],
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "chalk": "^2.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-font-magician": "^2.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.18.2",
    "stylelint": "^7.6.0",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.8.0",
    "svn-modules": "^0.1.8",
    "url-loader": "^0.5.7",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "demo"
    ],
    "testRegex": "\\.test\\.jsx?$"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1"
  }
}
